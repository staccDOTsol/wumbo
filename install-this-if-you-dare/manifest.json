{
  "manifest_version": 2,
  "name": "Ton in Twitter",
  "description": "An extension that allows you to use Ton in Twitter",
  "version": "1.0",
  "browser_action": {
    "default_title": "Ton in Twitter",
    "default_icon": {
      "16": "assets/img/icon16.png",
      "48": "assets/img/icon48.png",
      "128": "assets/img/icon128.png"
    }
  },
  "icons": {
    "16": "assets/img/icon16.png",
    "48": "assets/img/icon48.png",
    "128": "assets/img/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ],
  "web_accessible_resources": [
    "chunk-HZ3OUG7H.js"
  ],
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>"  ]
}